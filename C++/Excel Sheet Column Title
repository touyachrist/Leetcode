class Solution 
{
public:
    string convertToTitle(int n) 
    {
        string result;
        int m;
        while(n > 0)
        {
            m = n % 26;
            if(m == 0) m = 26;
            result = (char)(m + 65 - 1) + result;
            n = (n-m) / 26;
        }
        return result;
    }
};
